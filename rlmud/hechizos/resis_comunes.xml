<?xml version="1.0" encoding="iso8859-15"?>

<aliases>
  <alias
   match="^rg(\d+)$"
   enabled="y"
   group="Resistir Agua"
   regexp="y"
   send_to="10"
   sequence="100"
   temporary="y"
  >
  <send>rg y%1</send>
  </alias>
  <alias
   match="rg"
   enabled="y"
   expand_variables="y"
   group="Resistir Agua"
   send_to="10"
   sequence="100"
   temporary="y"
  >
  <send>rg @nombre</send>
  </alias>
  <alias
   match="rg *"
   enabled="y"
   group="Resistir Agua"
   sequence="100"
   temporary="y"
  >
  <send>formular resistir agua: %1</send>
  </alias>
</aliases>

