<?xml version="1.0" encoding="iso8859-15"?>

<aliases>
  <alias
   match="^gmi(\d+)$"
   enabled="y"
   group="Globo Mayor"
   regexp="y"
   send_to="10"
   sequence="100"
   temporary="y"
  >
  <send>gmi y%1</send>
  </alias>
  <alias
   match="gmi"
   enabled="y"
   expand_variables="y"
   group="Globo Mayor"
   send_to="10"
   sequence="100"
   temporary="y"
  >
  <send>gmi @nombre</send>
  </alias>
  <alias
   match="gmi *"
   enabled="y"
   group="Globo Mayor"
   sequence="100"
   temporary="y"
  >
  <send>formular globo mayor de invulnerabilidad: %1</send>
  </alias>
</aliases>

<aliases>
  <alias
   match="escm"
   enabled="y"
   sequence="100"
   temporary="y"
  >
  <send>formular escudo de energia</send>
  </alias>
</aliases>

<aliases>
  <alias
   match="^rpd(\d+)$"
   enabled="y"
   group="Repudiar"
   regexp="y"
   send_to="10"
   sequence="100"
   temporary="y"
  >
  <send>rpd m%1</send>
  </alias>
  <alias
   match="rpd"
   enabled="y"
   group="Repudiar"
   send_to="10"
   sequence="100"
   temporary="y"
  >
  <send>rpd m , m1 , m2 , m3 , m4 , m5 , m6 , m7 , m8 , m9</send>
  </alias>
  <alias
   match="rpd *"
   enabled="y"
   group="Repudiar"
   sequence="100"
   temporary="y"
  >
  <send>formular repudiar: %1</send>
  </alias>
</aliases>

