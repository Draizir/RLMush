<aliases>
  <!-- Hechizos varios -->
  <alias
   match="^wy(\d+)$"
   enabled="y"
   group="Conjuración"
   regexp="y"
   send_to="10"
   sequence="100"
   temporary="y"
  >
  <send>wy x%1</send>
  </alias>
