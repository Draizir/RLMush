<?xml version="1.0" encoding="iso8859-15"?>

<aliases>
  <alias
   match="^gni(\d+)$"
   enabled="y"
   group="Globo Menor"
   regexp="y"
   send_to="10"
   sequence="100"
   temporary="y"
  >
  <send>gni y%1</send>
  </alias>
  <alias
   match="gni"
   enabled="y"
   expand_variables="y"
   group="Globo Menor"
   send_to="10"
   sequence="100"
   temporary="y"
  >
  <send>gni @nombre</send>
  </alias>
  <alias
   match="gni *"
   enabled="y"
   group="Globo Menor"
   sequence="100"
   temporary="y"
  >
  <send>formular globo menor de invulnerabilidad: %1</send>
  </alias>
</aliases>

